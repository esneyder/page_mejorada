@font-face
	font-family 'f-menu'
	src url('../fonts-menu/icomoon.eot?lwipyc')
	src url('../fonts-menu/icomoon.eot?#iefixlwipyc') format('embedded-opentype'), url('../fonts-menu/icomoon.woff?lwipyc') format('woff'), url('../fonts-menu/icomoon.ttf?lwipyc') format('truetype'), url('../fonts-menu/icomoon.svg?lwipyc#icomoon') format('svg')
	font-weight normal
	font-style normal





body

	background-color #FFFFFF
	font-family  Open Sans,Helvetica Neue,Arial,sans-serif
	font-weight normal
	letter-spacing normal
	font-size 16px

	header
		background-color #5068DA
		width 100%
		height 3.75em
		position fixed
		top 0
		left 0


		.header-container
			width 95%
			width-max 65em


			.menu-button

				background-color #e33d25
				border-radius 3px
				padding .25em .625em
				color #FFFFFF
				text-decoration none
				transition background .35s ease-in,color .35s ease-in
                height 80px
                margin 0.75em

				.imenu:before
					font-family "f-menu"
					content "\e604"







